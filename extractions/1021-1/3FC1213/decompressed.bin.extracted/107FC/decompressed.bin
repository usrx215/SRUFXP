QListView::item:selected:!active { border-image: url(:/images/FocusBar_Selected.png); 
color: rgb(255, 180, 0); 
} 

QListView::item:selected:active { border-image: url(:/images/FocusBar.png); 
color: rgb(0, 0, 0); 
} 

color: rgb(255, 255, 255); 

QScrollBar:vertical {
                       border-image: url(:/images/Scrollbar_Bg.png);
                       width: 10px;
                       margin: 0px 0 0px 0;
                       }
QScrollBar::handle:vertical {
                            border-image: url(:/images/Scrollbar_Icon.png);
                             min-height: 38px;
                         }
QScrollBar::add-line:vertical {
                             height: 0px;
                             subcontrol-position: bottom;
                             subcontrol-origin: margin;
                         }
QScrollBar::sub-line:vertical {
                             height: 0px;
                             subcontrol-position: top;
                             subcontrol-origin: margin;
                         }
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
                           background: none;
                       };