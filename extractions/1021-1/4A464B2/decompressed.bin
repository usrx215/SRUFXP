            <tpl id="merch_collection.tpl">
              <div class="featuredcollection">
                <a titleid="{genreID}" class="collection" titleidx="{titleidx}" action="application.events.publish('loadgenreview', {id: {genreID}})">
                  <img src="{boxArtPrefix}"/>
                </a>
                <div class="title-text">
                  <table cellpadding="0" height="100%" width="100%" cellspacing="0">
