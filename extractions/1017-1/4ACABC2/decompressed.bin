    <control id="settingspanel">
      <layout>
        <div id="SettingsPanel">
          <tpl id="settingspanel.tpl">
            {test}
            <div>
              <div id="Activate" class="panel">
                <div id="activate_devicenotactive" class="notactive_@@BrowseView.SettingsPanel.isActivated()@@">
                  <p>Activate CinemaNow on this device.</p>
                  <a id="btnsettingsactivate" class="button" action="BrowseView.showActivate()" down="btnsettingsactivate">Activate Device</a>
                </div>
                <div id="activate_deviceactive" class="active_@@BrowseView.SettingsPanel.isActivated()@@">
                  <p>
                    CinemaNow is activated on this device.<br /><span class="grey">Go to </span><span class="url">www.cinemanow.com/device</span> <span class="grey"> to de-activate.</span>
                  </p>
                </div>
              </div>
              <div id="Updates" class="panel">
                <div>
                  <p class="white">Check for new CinemaNow updates.</p>
                  <a id="btnsettingsupdates" class="button" action="BrowseView.checkForUpdates()" down="btnsettingsupdates">Check for Updates</a>
                </div>
              </div>
              <div id="GiftCards" class="panel">
                <div>
                  <p class="white">Redeem gift card for account credit.</p>
                  <a id="btnsettingsgc" class="button" action="BrowseView.goGiftCard(false)" left="selectedslave" down="btnsettingsgc">Redeem Gift Card</a>
                </div>
              </div>
              <div id="Reset" class="panel">
                <div>
                  <a id="btnsettingsdefault" class="button" down="btnsettingsdefault">Reset Settings to Defaults</a>
                  <p>Resets all application settings to default values.</p>
                </div>
              </div>
              <div id="RequirePIN" class="panel">
                <div>
                  <div id="purchasepincheck" class="checkbox_@@BrowseView.SettingsPanel.requiresPurchasePin()@@">
                    <a id="btnsettingpin" action="BrowseView.SettingsPanel.togglePurchasePin()" down="btnsettingpin"> </a>
                  </div>
                  <div id="requirePinLabel">
                    <p class="white heading">Require PIN for purchases </p>
                    <div>
                      Requires a PIN to be entered when making <br />purchases.
                    </div>
                  </div>
                  <div id="requirePinHelpText">
                    You can also change these settings via the website.<br />Go to <span class="white url">www.cinemanow.com/parent</span> to configure parental controls or change settings.
                  </div>
                </div>
              </div>
              <div id="Parental" class="panel">
                <div>
                  <div id="parentcheck" class="checkbox_@@BrowseView.SettingsPanel.requiresParentalControls()@@">
                    <a id="btnsettingsparental" action="BrowseView.SettingsPanel.toggleParentalControls()" down="btnsettingsparentalstartup"> </a>
                  </div>
                  <div id="parentcheckonstartup" class="checkbox_@@BrowseView.SettingsPanel.requiresParentalControlsOnStartUp()@@">
                    <a id="btnsettingsparentalstartup" action="BrowseView.SettingsPanel.toggleParentalControlsOnStartUp()" up="btnsettingsparental" down="btnsettingsparentalstartup"> </a>
                  </div>

                  <div id="parentchecklabel">
                    <p id="pparentchecklabel" class="white heading">Parental Controls</p>
                    <div style="text-align:left">
                      Restrict access to content based on your <br />account settings.
                    </div>
                  </div>

                  <div id="parentchecklaunchlabel">
                    <p class="white heading">Automatically enable parental controls on launch</p>
                  </div>

                  <div id="requirePinHelpText">
                    You can also change these settings via the website.<br />Go to <span class="white url">www.cinemanow.com/parent</span> to configure parental controls or change settings.
                  </div>
                </div>
              </div>
              <div id="SpeedTest" class="panel">
                <div>
                  <a id="btnsettingsspeedtest" class="button" action="BrowseView.showSpeed()" down="btnsettingsspeedtest">Test Speed</a>
                  <p> </p>
                </div>
              </div>
              <div id="About" class="panel">
                <div>
                  <p class="white">About CinemaNow</p>
                  <p>App Version: {version}.{build}</p>
                  <p class="settingssdkversion">SDK Version: {Major}.{Minor}.{Revision}.{Branch}.{Build}</p>
                  <p>Device FW Version: {firmwareversion}</p>
                  <p>
                    CinemaNow Device Name: <span id="settingsdeviceid">{devicename}</span>
                  </p>
                  <p> Email Address: {email} </p>
                  <p> Device Model: {modelnumber}</p>
                  <p class="deviceid">
                    Device ID: <span id="settingsdeviceid">{deviceid}({affid}:{devicetype})</span>
                  </p>

                  <a id="btnsettingslegal" class="button" up="btnsettingprivacy" left="selectedslave" action="BrowseView.showLegalInfo('legal')">View Legal Info</a>
                  <a id="btnsettingseula" class="button" action="BrowseView.showEula('legal')" left="selectedslave" down="btnsettingstos">View EULA</a>
                  <a id="btnsettingstos" class="button" action="BrowseView.showTerms('terms')" left="selectedslave" down="btnsettingprivacy">View Terms of Service</a>
                  <a id="btnsettingprivacy" class="button" action="BrowseView.showPrivacy('privacy')" left="selectedslave" down="btnsettingslegal">View Privacy Policy</a>

                </div>
              </div>
              <div id="SystemStatus" class="panel">
                <div>
                  <div class="systemstatuscotainer">
                    <div class="systemstatusheader">System Status</div>
                    <div class="greytext">Curious about your bandwidth or experiencing trouble with your system?</div>
                    <div class="testinfo">
                      This test will determine the following:
                      <ul>
                        <li>Sustained bandwidth for video playback</li>
                        <li>Connection type (wired or wireless)</li>
                        <li>Device Status: Active/Inactive</li>
                        <li>Store Connection: Active/Inactive</li>
                        <li>IP Address</li>
                        <li>Registered Email Address</li>
                      </ul>
                      <a id="btnBeginSystemStatus" class="button" up="btnBeginSystemStatus" down="btnBeginSystemStatus" action="BrowseView.showSystemStatus()">Begin Test</a>
                    </div>
                  </div>
                  <div class="systemstatusresults">
                    <div class="systemstatusheader">System Status Check Results</div>
                    <div class="bwResults">
                      Sustained Bandwidth: <span class="theResult"> </span> MBPS
                    </div>
                    <div class="bwMessage">
                      <span class="theResult"> </span>
                    </div>
                    <div class="bwSummary">
                      <span class="theResult"> </span>
                    </div>
                    <div class="connectionResults">
                      Connection Type: <span class="theResult"> </span><span class="warning"> </span>
                    </div>
                    <div class="activationResults">
                      Device Status: <span class="theResult"> </span><span class="warning"> </span>
                    </div>
                    <div class="resResults">
                      Store Connection: <span class="theResult"> </sp